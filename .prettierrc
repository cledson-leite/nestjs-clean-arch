{
  "singleQuote": true,
  "trailingComma": "all",
  "plugins": ["prettier-plugin-organize-imports"],
    "organizeImports": {
    "groups": [
      ["builtin", "external"],
      "internal",
      "parent",
      "sibling",
      "object"
    ],
    "spaces": [2, 2],
    "order": {
      "builtin": "asc",
      "external": "asc",
      "internal": "asc",
      "parent": "asc",
      "sibling": "asc",
      "object": "asc"
    }
  }
}